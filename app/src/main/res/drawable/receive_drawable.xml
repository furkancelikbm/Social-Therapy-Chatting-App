<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <solid android:color="#33000000"></solid>
            <corners
                android:bottomRightRadius="45sp"
                android:topRightRadius="45dp"
                android:topLeftRadius="45dp">

            </corners>
            <stroke android:width="1dp" android:color="#6A8A8A98"></stroke>
        </shape>
    </item>
    <item android:right="2dp"
        android:left="2dp"
        android:top="4dp"
        >
        <shape>
            <solid android:color="#182881"></solid>
            <corners
                android:bottomRightRadius="45sp"
                android:topRightRadius="45dp"
                android:topLeftRadius="45dp">

            </corners>
        </shape>
    </item>

</layer-list>